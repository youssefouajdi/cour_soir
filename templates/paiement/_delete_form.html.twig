<form method="post" action="{{ path('paiement_delete', {'idPaiement': paiement.idPaiement}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ paiement.idPaiement) }}">
    <button class="btn">Delete</button>
</form>
